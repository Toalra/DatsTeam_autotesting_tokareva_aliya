    <script>
      var screenDisplay = new Vue({
        el: "#screenDisplay",
        data() {
          return {
            slides: [
              {
                picture: "img/products/display-datspush.png",
                site: "https://datspush.com",
                name: "datspush.com",
              },
              {
                picture: "img/products/display-leadbit.png",
                site: "https://leadbit.com/",
                name: "leadbit.com",
              },
            ],
          };
        },
        computed: {
          currentSlide() {
            return this.slides[0];
          },
        },
        methods: {
          changeSlide() {
            this.slides.push(this.slides.shift());
          },
        },
      });
    </script>
